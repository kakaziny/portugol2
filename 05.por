programa {
  funcao inicio() {
     inteiro num1
    inteiro num2

    escreva("Informe o primeiro número: ")
    leia(num1)

    escreva("Informe o segundo número: ")
    leia(num2)

    se(num1 > num2){
      escreva("O ", num1," é maior")
    }
    senao{
      escreva("O ", num2," é maior")
  }
  }
}